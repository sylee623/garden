@article{LynnEtAl2025,
  title = {Nonlinear Recurrent Inhibition through Facilitating Serotonin Release in the Raphe},
  author = {Lynn, Michael B. and Geddes, Sean D. and Chahrour, Mohamad and Maill{\'e}, S{\'e}bastien and {Caya-Bissonnette}, L{\'e}a and Harkin, Emerson and {Harvey-Girard}, {\'E}rik and {Haj-Dahmane}, Samir and Naud, Richard and B{\'e}{\"i}que, Jean-Claude},
  year = {2025},
  month = may,
  journal = {Nat Neurosci},
  volume = {28},
  number = {5},
  pages = {1024--1037},
  publisher = {Nature Publishing Group},
  issn = {1546-1726},
  doi = {10.1038/s41593-025-01912-7},
  urldate = {2025-07-13},
  abstract = {Serotonin (5-HT) neurons in the dorsal raphe nucleus (DRN) receive a constellation of long-range inputs, yet guiding principles of local circuit organization and underlying computations in this nucleus are largely unknown. Using inputs from the lateral habenula to interrogate the processing features of the mouse DRN, we uncovered 5-HT1A receptor-mediated recurrent connections between 5-HT neurons, refuting classical theories of autoinhibition. Cellular electrophysiology and imaging of a genetically encoded 5-HT sensor revealed that these recurrent inhibitory connections spanned the raphe, were slow, stochastic, strongly facilitating and gated spike output. These features collectively conveyed highly nonlinear dynamics to this network, generating excitation-driven inhibition and winner-take-all computations. In vivo optogenetic activation of lateral habenula inputs to DRN, at frequencies where these computations are predicted to ignite, transiently disrupted expression of a reward-conditioned response in an auditory conditioning task. Together, these data identify a core computation supported by an unsuspected slow serotonergic recurrent inhibitory network.},
  copyright = {2025 The Author(s), under exclusive licence to Springer Nature America, Inc.},
  langid = {english},
  keywords = {Dynamical systems,Neural circuits,Neurotransmitters,Short-term potentiation},
  file = {/Users/seoyeon/Zotero/storage/EMCNCJIN/Lynn et al. - 2025 - Nonlinear recurrent inhibition through facilitating serotonin release in the raphe.pdf}
}

@article{MalloryEtAl2025,
  title = {The Time Course and Organization of Hippocampal Replay},
  author = {Mallory, Caitlin S. and Widloski, John and Foster, David J.},
  year = {2025},
  month = jan,
  journal = {Science},
  volume = {387},
  number = {6733},
  pages = {541--548},
  publisher = {American Association for the Advancement of Science},
  doi = {10.1126/science.ads4760},
  urldate = {2025-02-11},
  abstract = {The mechanisms by which the brain replays neural activity sequences remain unknown. Recording from large ensembles of hippocampal place cells in freely behaving rats, we observed that replay content is strictly organized over multiple timescales and governed by self-avoidance. After movement cessation, replays avoided the animal's previous path for 3 seconds. Chains of replays avoided self-repetition over a shorter timescale. We used a continuous attractor model of neural activity to demonstrate that neuronal fatigue both generates replay sequences and produces self-avoidance over the observed timescales. In addition, replay of past experience became predominant later into the stopping period, in a manner requiring cortical input. These results indicate a mechanism for replay generation that unexpectedly constrains which sequences can be produced across time.},
  file = {/Users/zonaincerta/Library/Mobile Documents/iCloud~md~obsidian/Documents/second_brain_2024/3.Resources/_pdfs/Mallory et al. - 2025 - The time course and organization of hippocampal replay.pdf}
}

@article{MarchandEtAl2025,
  title = {Targeting Protein--Ligand Neosurfaces with a Generalizable Deep Learning Tool},
  author = {Marchand, Anthony and Buckley, Stephen and Schneuing, Arne and Pacesa, Martin and Elia, Maddalena and Gainza, Pablo and Elizarova, Evgenia and Neeser, Rebecca M. and Lee, Pao-Wan and Reymond, Luc and Miao, Yangyang and Scheller, Leo and Georgeon, Sandrine and Schmidt, Joseph and Schwaller, Philippe and Maerkl, Sebastian J. and Bronstein, Michael and Correia, Bruno E.},
  year = {2025},
