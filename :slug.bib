  author = {Hanchate, Naresh K. and Kondoh, Kunio and Lu, Zhonghua and Kuang, Donghui and Ye, Xiaolan and Qiu, Xiaojie and Pachter, Lior and Trapnell, Cole and Buck, Linda B.},
  year = {2015},
