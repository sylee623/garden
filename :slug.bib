@article{Aksoy-AkselEtAl2024,
  title = {Amygdala Intercalated Cells Form an Evolutionarily Conserved System Orchestrating Brain Networks},
  author = {{Aksoy-Aksel}, Ayla and Ferraguti, Francesco and Holmes, Andrew and L{\"u}thi, Andreas and Ehrlich, Ingrid},
  year = {2024},
  month = dec,
  journal = {Nat Neurosci},
  pages = {1--14},
  publisher = {Nature Publishing Group},
  issn = {1546-1726},
  doi = {10.1038/s41593-024-01836-8},
  urldate = {2025-01-15},
  abstract = {The amygdala attributes valence and emotional salience to environmental stimuli and regulates how these stimuli affect behavior. Within the amygdala, a distinct class of evolutionarily conserved neurons form the intercalated cell (ITC) clusters, mainly located around the boundaries of the lateral and basal nuclei. Here, we review the anatomical, physiological and molecular characteristics of ITCs, and detail the organization of ITC clusters and their connectivity with one another and other brain regions. We describe how ITCs undergo experience-dependent plasticity and discuss emerging evidence demonstrating how ITCs are innervated and functionally regulated by neuromodulatory systems. We summarize recent findings showing that experience alters the balance of activity between different ITC clusters, thereby determining prevailing behavioral output. Finally, we propose a model in which ITCs form a key system for integrating divergent inputs and orchestrating brain-wide circuits to generate behavioral states attuned to current environmental circumstances and internal needs.},
  copyright = {2024 Springer Nature America, Inc.},
  langid = {english},
  keywords = {Amygdala,Anxiety,Neural circuits},
  file = {/Users/zonaincerta/Library/Mobile Documents/iCloud~md~obsidian/Documents/second_brain_2024/3.Resources/_pdfs/Aksoy-Aksel et al. - 2024 - Amygdala intercalated cells form an evolutionarily conserved system orchestrating brain networks.pdf}
}

