@article{SquairEtAl2023,
  title = {Recovery of Walking after Paralysis by Regenerating Characterized Neurons to Their Natural Target Region},
  author = {Squair, Jordan W. and Milano, Marco and De Coucy, Alexandra and Gautier, Matthieu and Skinnider, Michael A. and James, Nicholas D. and Cho, Newton and Lasne, Anna and Kathe, Claudia and Hutson, Thomas H. and Ceto, Steven and Baud, Laetitia and Galan, Katia and Aureli, Viviana and Laskaratos, Achilleas and Barraud, Quentin and Deming, Timothy J. and Kohman, Richie E. and Schneider, Bernard L. and He, Zhigang and Bloch, Jocelyne and Sofroniew, Michael V. and Courtine, Gregoire and Anderson, Mark A.},
  year = {2023},
  month = sep,
  journal = {Science},
  volume = {381},
  number = {6664},
  pages = {1338--1345},
  issn = {0036-8075, 1095-9203},
  doi = {10.1126/science.adi6412},
  urldate = {2025-01-13},
  abstract = {Axon regeneration can be induced across anatomically complete spinal cord injury (SCI), but robust functional restoration has been elusive. Whether restoring neurological functions requires directed regeneration of axons from specific neuronal subpopulations to their natural target regions remains unclear. To address this question, we applied projection-specific and comparative single-nucleus RNA sequencing to identify neuronal subpopulations that restore walking after incomplete SCI. We show that chemoattracting and guiding the transected axons of these neurons to their natural target region led to substantial recovery of walking after complete SCI in mice, whereas regeneration of axons simply across the lesion had no effect. Thus, reestablishing the natural projections of characterized neurons forms an essential part of axon regeneration strategies aimed at restoring lost neurological functions.           ,              Editor's summary                            Although several experimental approaches have shown positive results in axonal regeneration after spinal cord injury (SCI), complete recovery of motor functions remains an elusive target. Squair               et al               . hypothesized that restoration of complete axonal projection of a selected neuronal population to their natural target could promote better functional recovery. After using single-cell RNA sequencing to identify the most promising neuronal population, the authors showed that promoting axonal growth and path guidance to their natural target in this population restored walking in mice after complete SCI. By contrast, broad axonal restoration across the lesion had no effect, suggesting that a more targeted approach is necessary for functional recovery after SCI. ---Mattia Maroso                        ,              Restoring axonal projections of selected neurons to their target is critical for functional recovery after spinal cord injury in mice.},
  langid = {english},
  file = {/Users/zonaincerta/Library/Mobile Documents/iCloud~md~obsidian/Documents/second_brain_2024/3.Resources/_pdfs/Squair et al. - 2023 - Recovery of walking after paralysis by regenerating characterized neurons to their natural target re 1.pdf;/Users/zonaincerta/Library/Mobile Documents/iCloud~md~obsidian/Documents/second_brain_2024/3.Resources/_pdfs/Squair et al. - 2023 - Recovery of walking after paralysis by regenerating characterized neurons to their natural target re.pdf}
}

@article{SubriniEtAl2025,
  title = {Systematic Identification of {{Y-chromosome}} Gene Functions in Mouse Spermatogenesis},
  author = {Subrini, Jeremie and Varsally, Wazeer and Balsells, Irina Balaguer and Bensberg, Maike and Sioutas, Georgios and Ojarikre, Obah and Maciulyte, Valdone and Gylemo, Bj{\"o}rn and Crawley, Katharine and Courtis, Katherine and {de Rooij}, Dirk G. and Turner, James M. A.},
  year = {2025},
  month = jan,
  journal = {Science},
  volume = {387},
  number = {6732},
  pages = {393--400},
  publisher = {American Association for the Advancement of Science},
  doi = {10.1126/science.ads6495},
  urldate = {2025-02-11},
  abstract = {The mammalian Y chromosome is essential for male fertility, but which Y genes regulate spermatogenesis is unresolved. We addressed this by generating 13 Y-deletant mouse models. In Eif2s3y, Uty, and Zfy2 deletants, spermatogenesis was impaired. We found that Uty regulates spermatogonial proliferation, revealed a role for Zfy2 in promoting meiotic sex chromosome pairing, and uncovered unexpected effects of Y genes on the somatic testis transcriptome. In the remaining single Y-gene deletants, spermatogenesis appeared unperturbed, but testis transcription was still altered. Multigene deletions, including a human-infertility AZFa model, exhibited phenotypes absent in single Y deletants. Thus, Y genes may regulate spermatogenesis even if they show no phenotypes when deleted individually. This study advances our knowledge of Y evolution and infertility and provides a resource to dissect Y-gene functions in other tissues.},
  file = {/Users/zonaincerta/Library/Mobile Documents/iCloud~md~obsidian/Documents/second_brain_2024/3.Resources/_pdfs/Subrini et al. - 2025 - Systematic identification of Y-chromosome gene functions in mouse spermatogenesis.pdf}
}

