@article{OKeeffeEtAl2025,
  title = {Typical Development of Synaptic and Neuronal Properties Can Proceed without Microglia in the Cortex and Thalamus},
  author = {O'Keeffe, Mary and Booker, Sam A. and Walsh, Darren and Li, Mosi and Henley, Chloe and {Sim{\~o}es de Oliveira}, Laura and Liu, Mingshan and Wang, Xingran and Banqueri, Maria and Ridley, Katherine and Dissanayake, Kosala N. and {Martinez-Gonzalez}, Cristina and Craigie, Kirsty J. and Vasoya, Deepali and Leah, Tom and He, Xin and Hume, David A. and Duguid, Ian and Nolan, Matthew F. and Qiu, Jing and Wyllie, David J. A. and Dando, Owen R. and {Gonzalez-Sulser}, Alfredo and Gan, Jian and Pridans, Clare and Kind, Peter C. and Hardingham, Giles E.},
  year = {2025},
  month = feb,
  journal = {Nat Neurosci},
  volume = {28},
  number = {2},
  pages = {268--279},
  publisher = {Nature Publishing Group},
  issn = {1546-1726},
  doi = {10.1038/s41593-024-01833-x},
  urldate = {2025-02-11},
  abstract = {Brain-resident macrophages, microglia, have been proposed to have an active role in synaptic refinement and maturation, influencing plasticity and circuit-level connectivity. Here we show that several neurodevelopmental processes previously attributed to microglia can proceed without them. Using a genetically modified mouse that lacks microglia (Csf1r∆FIRE/∆FIRE), we find that intrinsic properties, synapse number and synaptic maturation are largely normal in the hippocampal CA1 region and somatosensory cortex at stages where microglia have been implicated. Seizure susceptibility and hippocampal-prefrontal cortex coherence in awake behaving animals, processes that are disrupted in mice deficient in microglia-enriched genes, are also normal. Similarly, eye-specific segregation of inputs into the lateral geniculate nucleus proceeds normally in the absence of microglia. Single-cell and single-nucleus transcriptomic analyses of neurons and astrocytes did not uncover any substantial perturbation caused by microglial absence. Thus, the brain possesses remarkable adaptability to execute developmental synaptic refinement, maturation and connectivity in the absence of microglia.},
  copyright = {2025 The Author(s)},
  langid = {english},
  keywords = {Microglia,Neuronal development,Neurotransmitters,Synaptic development},
  file = {/Users/zonaincerta/Library/Mobile Documents/iCloud~md~obsidian/Documents/second_brain_2024/3.Resources/_pdfs/O’Keeffe et al. - 2025 - Typical development of synaptic and neuronal properties can proceed without microglia in the cortex.pdf}
}

@article{OngaroEtAl2025,
  title = {Muscle-Derived Myostatin Is a Major Endocrine Driver of Follicle-Stimulating Hormone Synthesis},
  author = {Ongaro, Luisina and Zhou, Xiang and Wang, Ying and Schultz, Hailey and Zhou, Ziyue and Buddle, Evan R. S. and Br{\^u}l{\'e}, Emilie and Lin, Yeu-Farn and Schang, Gauthier and Hagg, Adam and Castonguay, Roselyne and Liu, Yewei and Su, Gloria H. and Seidah, Nabil G. and Ray, Kevin C. and Karp, Seth J. and Boehm, Ulrich and {Ruf-Zamojski}, Frederique and Sealfon, Stuart C. and Walton, Kelly L. and Lee, Se-Jin and Bernard, Daniel J.},
