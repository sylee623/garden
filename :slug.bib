  langid = {english},
  keywords = {Cancer genomics,Transcriptomics},
  file = {/Users/seoyeon/Zotero/storage/68DTNRAL/Zheng et al. - 2017 - Massively parallel digital transcriptional profiling of single cells.pdf}
}

@article{ZichoEtAl2025,
  title = {Identification of the Subventricular Tegmental Nucleus as Brainstem Reward Center},
  author = {Zich{\'o}, Kriszti{\'a}n and Balog, Boldizs{\'a}r Zsolt and Sebest{\'e}ny, R{\'e}ka Z. and Brunner, J{\'a}nos and Tak{\'a}cs, Vir{\'a}g and Barth, Albert M. and Seng, Charlotte and Orosz, {\'A}ron and Aliczki, Man{\'o} and Seb{\H o}k, Hunor and Mikics, Eva and F{\"o}ldy, Csaba and Szabadics, J{\'a}nos and Nyiri, G{\'a}bor},
  year = {2025},
  month = jan,
  journal = {Science},
  volume = {387},
  number = {6732},
  pages = {eadr2191},
  publisher = {American Association for the Advancement of Science},
  doi = {10.1126/science.adr2191},
  urldate = {2025-02-11},
  abstract = {Rewards are essential for motivation, decision-making, memory, and mental health. We identified the subventricular tegmental nucleus (SVTg) as a brainstem reward center. In mice, reward and its prediction activate the SVTg, and SVTg stimulation leads to place preference, reduced anxiety, and accumbal dopamine release. Mice self-stimulate the SVTg, which can also be activated directly by the neocortex, resulting in effective inhibition of the lateral habenula, a region associated with depression. This mechanism may also explain why SVTg suppression induces aversion and increases fear. The translational relevance of these findings is supported by evidence in the rat, monkey, and human brainstem, establishing SVTg as a key hub for reward processing, emotional valence, and motivation.},
  file = {/Users/zonaincerta/Library/Mobile Documents/iCloud~md~obsidian/Documents/second_brain_2024/3.Resources/_pdfs/Zichó et al. - 2025 - Identification of the subventricular tegmental nucleus as brainstem reward center.pdf}
}

@article{ZongEtAl2025,
  title = {Hippocampal Output Suppresses Orbitofrontal Cortex Schema Cell Formation},
  author = {Zong, Wenhui and Zhou, Jingfeng and Gardner, Matthew P. H. and Zhang, Zhewei and Costa, Kau{\^e} Machado and Schoenbaum, Geoffrey},
  year = {2025},
  month = may,
  journal = {Nat Neurosci},
  volume = {28},
  number = {5},
  pages = {1048--1060},
  publisher = {Nature Publishing Group},
  issn = {1546-1726},
  doi = {10.1038/s41593-025-01928-z},
  urldate = {2025-07-13},
  abstract = {Both the orbitofrontal cortex (OFC) and the hippocampus (HC) are implicated in the formation of cognitive maps and their generalization into schemas. However, how these areas interact in supporting this function remains unclear, with some proposals supporting a serial model in which the OFC draws on task representations created by the HC to extract key behavioral features and others suggesting a parallel model in which both regions construct representations that highlight different types of information. In the present study, we tested between these two models by asking how schema correlates in rat OFC would be affected by inactivating the output of the HC, after learning and during transfer across problems. We found that the prevalence and content of schema correlates were unaffected by inactivating one major HC output area, the ventral subiculum, after learning, whereas inactivation during transfer accelerated their formation. These results favor the proposal that the OFC and HC operate in parallel to extract different features defining cognitive maps and schemas.},
  copyright = {2025 This is a U.S. Government work and not under copyright protection in the US; foreign copyright protection may apply},
  langid = {english},
  keywords = {Cognitive neuroscience,Reward},
  file = {/Users/seoyeon/Zotero/storage/MBCGGZUG/Zong et al. - 2025 - Hippocampal output suppresses orbitofrontal cortex schema cell formation.pdf}
}

