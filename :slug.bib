@article{LovelandEtAl2025,
  title = {A Single Gene Orchestrates Androgen Variation Underlying Male Mating Morphs in Ruffs},
  author = {Loveland, Jasmine L. and Zemella, Alex and Jovanovi{\'c}, Vladimir M. and M{\"o}ller, Gabriele and Sager, Christoph P. and Bastos, B{\'a}rbara and Dyar, Kenneth A. and Fusani, Leonida and Gahr, Manfred and {Giraldo-Deck}, Lina M. and Goymann, Wolfgang and Lank, David B. and Tokarz, Janina and Nowick, Katja and K{\"u}pper, Clemens},
